n        <div>Content {{item.index}}</div>\r\n      </a-list-item>\r\n    </virtual-scroller>\r\n    <a-spin v-if=\"loading\" class=\"demo-loading\" />\r\n  </a-list>\r\n</template>\r\n<script>\r\n  import reqwest from 'reqwest';\r\n  import infiniteScroll from 'vue-infinite-scroll';\r\n  import { DynamicScrollerItem } from 'vue-virtual-scroller';\r\n  const fakeDataUrl = 'https://randomuser.me/api/?results=10&inc=name,gender,email,nat&noinfo';\r\n  export default {\r\n    directives: { infiniteScroll },\r\n    data() {\r\n      return {\r\n        data: [],\r\n        loading: false,\r\n        busy: false,\r\n      };\r\n    },\r\n    beforeMount() {\r\n      this.fetchData(res => {\r\n        this.data = res.results.map((item, index) => ({ ...item, index }));\r\n      });\r\n    },\r\n    methods: {\r\n      fetchData(callback) {\r\n        reqwest({\r\n          url: fakeDataUrl,\r\n          type: 'json',\r\n          method: 'get',\r\n          contentType: 'application/json',\r\n          success: res => {\r\n            callback(res);\r\n          },\r\n        });\r\n      },\r\n      handleInfiniteOnLoad() {\r\n        const data = this.data;\r\n        this.loading = true;\r\n        if (data.length > 100) {\r\n          this.$message.warning('Infinite List loaded all');\r\n          this.busy = true;\r\n          this.loading = false;\r\n          return;\r\n        }\r\n        this.fetchData(res => {\r\n          this.data = data.concat(res.results).map((item, index) => ({ ...item, index }));\r\n          this.loading = false;\r\n        });\r\n      },\r\n    },\r\n    components: {\r\n      'virtual-scroller': VirtualScroller,\r\n    },\r\n  };\r\n</script>\r\n<style>\r\n  .demo-loading {\r\n    position: absolute;\r\n    bottom: 40px;\r\n    width: 100%;\r\n    text-align: center;\r\n  }\r\n</style>"]}]}